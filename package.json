{
  "name": "@cokeeps/travel-rule-sdk",
  "version": "0.0.4",
  "description": "Travel Rule SDK for VASPs to exchange required originator/beneficiary data securely and meet global compliance requirements.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "package.json",
    "tsconfig.json",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsdown",
    "prebuild": "rm -rf dist",
    "prepare": "test -d dist || npm run build",
    "prepublishOnly": "npm run clean && npm run build && npm run verify-build",
    "verify-build": "node scripts/verify-build.js",
    "widget": "npm run build && vite",
    "widget:dev": "npm run build && vite",
    "widget:build": "npm run build && vite build",
    "dev": "tsdown --watch",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts,.tsx,.vue",
    "lint:fix": "eslint . --ext .js,.cjs,.mjs,.ts,.tsx,.vue --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "keywords": [
    "travel",
    "travel-rule",
    "sdk",
    "vasp",
    "crypto",
    "compliance",
    "ivms101",
    "typescript"
  ],
  "author": {
    "name": "CoKeeps",
    "email": "your-email@example.com",
    "url": "https://github.com/CoKeeps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoKeeps/travel-rule-sdk.git"
  },
  "homepage": "https://github.com/CoKeeps/travel-rule-sdk#readme",
  "bugs": {
    "url": "https://github.com/CoKeeps/travel-rule-sdk/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.4.2",
    "vue-eslint-parser": "^9.4.3",
    "ts-node": "^10.9.0",
    "tsdown": "^0.20.0-beta.2",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vue": "^3.5.26",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "jose": "^6.1.3",
    "uuid": "^13.0.0",
    "zod": "^4.3.4"
  }
}
