{
    "name": "travel-sdk",
    "version": "1.0.0",
    "type": "module",
    "description": "SDK for travel rules",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "src",
        "scripts",
        "package.json",
        "tsconfig.json",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "prebuild": "rm -rf dist",
        "postinstall": "npm run build",
        "widget": "npm run build && vite",
        "widget:dev": "npm run build && vite",
        "widget:build": "npm run build && vite build",
        "dev": "tsc --watch"
    },
    "keywords": [
        "travel",
        "sdk"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yourusername/travel-sdk.git"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@vitejs/plugin-vue": "^5.2.4",
        "ts-node": "^10.9.0",
        "typescript": "^5.0.0",
        "vite": "^5.0.0",
        "vue": "^3.5.26",
        "vue-tsc": "^1.8.27"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "jose": "^6.1.3",
        "uuid": "^13.0.0",
        "zod": "^4.3.4"
    }
}
